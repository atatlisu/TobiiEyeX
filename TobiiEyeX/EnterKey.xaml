<UserControl x:Class="TobiiEyeX.EnterKey"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TobiiEyeX"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" HorizontalAlignment="Left" VerticalAlignment="Top" Width="123" Height="54">
    <Grid>
        <Image x:Name="image" Height="56" Margin="0,0,0,2" Width="123" Source="img/Keyboard/enter.png"/>
        <Label x:Name="label" Content="Enter" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
