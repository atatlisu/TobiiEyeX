        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="794*"/>
            <ColumnDefinition Width="0*"/>
        </Grid.ColumnDefinitions>
        <Image x:Name="image" Source="img/calibration_image.jpg" Stretch="Fill"/>
        <TextBlock x:Name="textBlockCorner" HorizontalAlignment="Left" Margin="485,316,0,0" TextWrapping="Wrap" Text="text" VerticalAlignment="Top" Height="88.861" Width="987" FontSize="36" UseLayoutRounding="False"/>
