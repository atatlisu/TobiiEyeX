<UserControl x:Class="TobiiEyeX.RightShiftKey"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TobiiEyeX"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" HorizontalAlignment="Left" VerticalAlignment="Top" Width="150" Height="54">
    <Grid>
        <Image x:Name="image" Height="54" Margin="0" Width="150" Source="img/Keyboard/rightShift.png"/>
        <Label x:Name="label" Content="Shift" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
